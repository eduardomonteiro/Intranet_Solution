@model IEnumerable<Intranet.Data.Entities.Diretoria>

<section class="content-diretoria">
    <div class="container">
        <div class="row">
            <div class="list-diretoria">
                @foreach (var item in Model)
                {
                    <div class="col l4 m6 s12">
                        <div class="item">
                            <img src="~/Content/Diretoria/@item.Foto" alt="">
                            <div class="info">
                                <p class="title">@item.Nome</p>
                                <p class="subtit">@item.Cargo</p>
                                @*<p class="subtit">Nome do shopping</p>*@
                                <p>@item.Descricao</p>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</section>
